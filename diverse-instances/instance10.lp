% mark m cells of an n x n grid
#const n=3.
#const m=7.

% the marked cells should be all together i.e. they all should be reachable from any other
{ x((1..n,1..n)) } = m.

connect(C) :- x(C), #false: x(C'), C'<C.
connect((X',Y')) :- connect((X,Y)), x((X',Y')), |X-X'|+|Y-Y'|=1.

:- x(C), not connect(C).

#show x/1.

#show weight/2.

